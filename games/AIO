-- Fungsi utama untuk menjalankan script
local function main()
    if not kickPlayerIfUnsupportedGame() then
        return
    end

    local isGameSupported = isSupportedGame()

    if isGameSupported then
        local gameInfo = getGameInfo()
        if not gameInfo or not gameInfo.guiName or not gameInfo.tabs or not gameInfo.functions then
            game.Players.LocalPlayer:Kick("Error loading game info. Please check your configuration.")
        else
            -- Hentikan animasi checking games
            stopCheckingAnimation()
            loadingText.Text = "Games Found | " .. gameInfo.guiName
            wait(2) -- Tampilkan pesan selama 2 detik

            -- Hapus GUI loading dengan animasi
            local tweenOut = game:GetService("TweenService"):Create(loadingFrame, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Position = UDim2.new(0.35, 0, 0.55, 0)})
            tweenOut:Play()
            tweenOut.Completed:Connect(function()
                loadingGUI:Destroy()
                createKeyInputGUI(function(inputKey)
                    if verifyKey(inputKey) then
                        -- Tunggu 2 detik untuk menampilkan pesan sukses, kemudian buat GUI utama
                        wait(2)
                        local GUI = Mercury:Create{
                            Name = gameInfo.guiName,
                            Size = UDim2.fromOffset(600, 400),
                            Theme = Mercury.Themes.Dark,
                            Link = gameInfo.guiName -- Gantilah dengan tautan yang sesuai
                        }

                        -- Menambahkan bagian notifikasi dan kredit yang diminta
                        GUI:Notification{
                            Title = "CanCantHUB | " .. gameInfo.guiName,
                            Text = "Successfully Loaded CanCantHUB | " .. gameInfo.guiName,
                            Duration = 3,
                            Callback = function() end
                        }
                        GUI:Credit{
                            Name = "Gi",
                            Description = "CanCantHUB | " .. gameInfo.guiName,
                            Discord = "https://discord.gg/DButHVVX9Y"
                        }

                        gameInfo.functions(GUI, gameInfo.tabs)
                        return true
                    else
                        return false
                    end
                end)
            end)
        end
    end
end

-- Panggil fungsi utama
main()
